[alias]
	aco = add -A && commit -m
	a = "!git add ."
	d = diff
	co = checkout
	ci = commit
	ps = "!git push origin $(git rev-parse --abbrev-ref HEAD)"
	pl = "!git pull origin $(git rev-parse --abbrev-ref HEAD)"
	st = status
	br = branch
[pull]
  rebase = false
[core]
  editor = nvim
  ignorecase = false
[github]
  user = gonstoll
